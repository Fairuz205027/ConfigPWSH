{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf6cf ",
          "foreground": "#ff66cc",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "template": " {{ .UserName }} ",
          "foreground": "#ffffff",
          "background": "#ff66cc",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf07c {{ .Path }} ",
          "foreground": "#c77dff",
          "powerline_symbol": "\ue0b0",
          "background": "#ff66cc",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ .HEAD }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#c77dff",
          "type": "git",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "austin",
            "threshold": 500
          },
          "template": " \uf252 {{ .FormattedMs }} ",
          "foreground": "#c77dff",
          "powerline_symbol": "\ue0b0",
          "background": "#ff66cc",
          "type": "executiontime",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf017 {{ .CurrentDate | date .Format \"15:04:05\" }} ",
          "foreground": "#c77dff",
          "type": "time",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u251c\u2500",
          "foreground": "#ff66cc",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "charged_icon": "\uf240",
            "discharge_icon": "\uf0e7"
          },
          "template": " HP:{{ .Percentage }}% ",
          "foreground": "#ffffff",
          "background": "#c77dff",
          "type": "battery",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2500",
          "foreground": "#ff66cc",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "memory": "physical"
          },
          "template": " MP:{{ .Memory }} ",
          "foreground": "#ff66cc",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2500",
          "foreground": "#ff66cc",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " COMBO:x{{ if .Env.COMBO_COUNT }}{{ .Env.COMBO_COUNT }}{{ else }}0{{ end }} ",
          "foreground": "#c77dff",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2500\u2524",
          "foreground": "#ff66cc",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\ue0b2",
          "foreground": "#c77dff",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \u685c ",
          "foreground": "#ffffff",
          "background": "#c77dff",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\ue0b4",
          "foreground": "#c77dff",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " ‚ùØ ",
          "foreground": "#ff66cc",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}